https://localhost:8443 {
    root * ./origin8443
    file_server

    #header Cross-Origin-Embedder-Policy "require-corp"
    #header Cross-Origin-Opener-Policy "same-origin"


    header Content-Security-Policy "worker-src 'self' https://localhost:9443"
    # header Content-Security-Policy "worker-src 'self'"  # Blocks load from other

    # Ensure JS files are served with correct MIME type
    @jsFiles {
        path *.js
    }
    header @jsFiles Content-Type "text/javascript"

}

https://localhost:9443 {
    root * ./origin9443
    file_server

    # Must match the main originâ€™s COOP/COEP
    #header Cross-Origin-Embedder-Policy "require-corp"
   # header Cross-Origin-Opener-Policy "same-origin"
    #header Cross-Origin-Resource-Policy "cross-origin"

    #header Content-Security-Policy: worker-src 'none';

    # CORS (required only if you're loading the worker cross-origin)
    header Access-Control-Allow-Origin "https://localhost:8443"
    #header Access-Control-Allow-Methods "GET, OPTIONS"
}

